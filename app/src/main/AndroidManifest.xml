<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.neu.madcourse.numad21s_yutinggan">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_yg"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_yg_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.NUMAD21SYutingGan">
        <activity android:name=".MainActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            android:configChanges="orientation|screenSize"
        </activity>
        <activity
            android:configChanges="orientation|screenSize"
            android:name=".AboutMeActivity"
            android:label="@string/title_activity_about_me"
            android:theme="@style/Theme.NUMAD21SYutingGan.NoActionBar"></activity>
        <activity
            android:configChanges="orientation|screenSize"
            android:name=".ClickyActivity"
            android:label="@string/title_activity_clicky"
            android:theme="@style/Theme.NUMAD21SYutingGan.NoActionBar" />
    </application>

</manifest>